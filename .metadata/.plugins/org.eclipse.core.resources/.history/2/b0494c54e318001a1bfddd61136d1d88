package com.test.threadSample;

public class SingleThreadDemo extends Thread {
	public void run() {
		try {
			// System.out.println("Thread : " + Thread.currentThread().getId() +
			// " : Is Running");
			System.out.println("Thread : " + Thread.currentThread().getName() + " : Is Running");
		} catch (Exception e) {
			System.out.println(Thread.currentThread().getId() + " : ThreadRunning - Interrupted");
			// System.out.println(" === Thread Running - Interrupted === ");
		}
	}
}
